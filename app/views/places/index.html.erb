<h1>Listing places</h1>

<table>
  <tr>
    <th>Area</th>
    <th>Name</th>
    <th>Category</th>
    <th>Phone</th>
    <th>Email</th>
    <th>Www</th>
    <th>Stars</th>
    <th>Geo location</th>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @places.each do |place| %>
  <tr>
    <td><%= place.area_id %></td>
    <td><%= place.name %></td>
    <td><%= place.category %></td>
    <td><%= place.phone %></td>
    <td><%= place.email %></td>
    <td><%= place.www %></td>
    <td><%= place.stars %></td>
    <td><%= place.geo_location %></td>
    <td><%= place.description %></td>
    <td><%= link_to 'Show', place %></td>
    <td><%= link_to 'Edit', edit_place_path(place) %></td>
    <td><%= link_to 'Destroy', place, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Place', new_place_path %>
